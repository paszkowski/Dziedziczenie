package animal;

public class Main {

	public static void main(String[] args) {
		Burd ptak = new Burd();
		ptak.color = "Niebieski";
				
		Cat kot = new Cat();
		kot.color = "Zielony";
		
		System.out.println("Zwierzaki daja glos!");
		System.out.println("Ptak daje glos: ");
		ptak.dajGlos();
		System.out.println("Kot daje glos");
		kot.dajGlos();

	}

}
